{
  "name": "SQLite Recovery Tool",
  "description": "Web-based GUI tool for recovering data from corrupt SQLite database files",
  "type": "docker",
  "port": 5000,
  "healthcheck": {
    "path": "/api/health",
    "interval": 30,
    "timeout": 10,
    "retries": 3
  },
  "environment": {
    "NODE_ENV": "production",
    "PORT": "5000"
  },
  "volumes": [
    {
      "source": "uploads",
      "target": "/app/uploads",
      "type": "volume"
    }
  ],
  "labels": {
    "coolify.managed": "true",
    "coolify.type": "application"
  }
}
